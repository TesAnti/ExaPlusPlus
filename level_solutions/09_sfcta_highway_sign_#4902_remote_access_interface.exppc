GRAB 300
LINK 800
REPL print
; WHILE2
MARK WHILE2_START
TEST X < 27
FJMP WHILE2_END
COPY X M
COPY F M
ADDI X 1 T
COPY T X
JUMP WHILE2_START
MARK WHILE2_END
COPY 9999 M
WIPE
HALT
MARK print
; FOREVER0
MARK FOREVER0_START
COPY M X
; IF1
TEST X > 27
FJMP IF1_END
JUMP FOREVER0_END
JUMP IF1_END
MARK IF1_END
DIVI X 9 T
COPY T #DATA
MODI X 9 T
COPY T #DATA
COPY M #DATA
JUMP FOREVER0_START
MARK FOREVER0_END
HALT

